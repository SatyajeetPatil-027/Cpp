//addition of two numbers using functions

#include<iostream>
using namespace std;

void addition(int &n1,int &n2)
{
	cout<<"The answer is:"<<n1+n2<<endl;
}

int main()
{
	int a;
	cout<<"Enter the value of a:"<<endl;
	cin>>a;
	int b;
	cout<<"Enter the value of b:"<<endl;
	cin>>b;
	
	addition(a,b);
	return 0;
}
